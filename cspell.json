{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "files": ["src/**/*"],
  "ignorePaths": ["*.snap", "*.codegen.*", "codegen.ts", "src/i18n/i18n-*"],
  "words": [
    "bookseries",
    "codegen",
    "followee",
    "followees",
    "henken",
    "henkens",
    "reexecute",
    "typesafe",
    "unfollow"
  ],
  "ignoreWords": ["fontawesome", "fortawesome", "urql", "nextjs", "clsx"]
}
